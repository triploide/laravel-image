{
    "name": "kamecode/image",
    "description": "Image manipulation for Laravel",
    "keywords": ["laravel", "image", "intervention"],
    "license": "MIT",
    "authors": [
        {
            "name":     "KameCode",
            "email":    "info@kame-code.com",
            "homepage": "https://kame-code.com"
        }
    ],
    "require": {
        "php": "^7.3|^7.4|^8.0",
        "illuminate/console": "^9.0",
        "illuminate/filesystem": "^9.0",
        "illuminate/support": "^9.0",
        "intervention/image": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "KameCode\\Image\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KameCode\\Image\\KameImageServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0@dev"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
